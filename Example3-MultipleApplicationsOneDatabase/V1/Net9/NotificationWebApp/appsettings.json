{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      //"System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceBricks": {
    "Application": {
      "Name": "ServiceBricks Example3 Notification Website"
    },
    "Cache": {
      "Semaphore": {
        "DelayMilliseconds": 3000,
        "CancellationMilliseconds": 20000,
        "OrphanTimeoutMilliseconds": 10000
      },
      // This is used when the Send Timer is enabled. See AddServiceBricksCacheClientForService() in startup.
      // It uses a distributed semaphore for queue processing as the number of service instances scale.
      // Alternatively, you can host the full cache microservice for direct database access, rather than using the network.
      "Client": {
        "Api": {
          "BaseServiceUrl": "https://localhost:7002/api/v1.0",
          "TokenUrl": "https://localhost:7000/api/v1.0/Security/Authentication/AuthenticateUserAsync",
          "TokenType": "password",
          "TokenClient": "unittest@servicebricks.com",
          "TokenSecret": "UnitTest123!@#"
        }
      }
    },
    "Logging": {
      "WebRequestMessage": {
        "EnableLogging": false,
        "EnableExceptions": true,
        "EnableUserStorageKey": true,
        "EnableRequestIPAddress": true,
        "EnableRequestBody": true,
        "EnableRequestBodyOnError": true,
        "EnableRequestProtocol": true,
        "EnableRequestScheme": true,
        "EnableRequestMethod": true,
        "EnableRequestPath": true,
        "EnableRequestPathBase": true,
        "EnableRequestQueryString": true,
        "EnableRequestQuery": true,
        "EnableRequestRouteValues": true,
        "EnableRequestHost": true,
        "EnableRequestHasFormContentType": true,
        "EnableRequestCookies": true,
        "EnableRequestContentType": true,
        "EnableRequestContentLength": true,
        "EnableRequestHeaders": true,
        "EnableRequestIsHttps": true,
        "EnableResponseStatusCode": true,
        "EnableResponseHeaders": true,
        "EnableResponseCookies": true,
        "EnableResponseContentType": true,
        "EnableResponseContentLength": true,
        "EnableResponseTotalMilliseconds": true,
        "EnableResponseBody": true,
        "EnableExcludeRequestPathsRegExExpressions": false,
        "ExcludeRequestPaths": [
          "/css/",
          "/img/",
          "/js/",
          "/lib/",
          "/webfonts/"
        ],
        "EnableExcludeIpAddressesRegExExpressions": false,
        "ExcludeIpAddresses": [
          // "127.0.0.1",
          // "::1"
        ]
      }
    },
    "Notification": {
      "Send": {
        "TimerEnabled": false,
        "TimerIntervalMilliseconds": 7000,
        "TimerDueMilliseconds": 1000,
        "EmailFromDefault": "support@servicebricks.com",
        "SmsFromDefault": "1234567890",
        "IsDevelopment": false,
        "DevelopmentEmailTo": "developer@servicebricks.com",
        "DevelopmentSmsTo": "1234567890"
      },
      "Smtp": {
        "EmailServer": "yourserver.com",
        "EmailPort": 123,
        "EmailEnableSsl": true,
        "EmailUsername": "username",
        "EmailPassword": "password"
      }
    },
    "Security": {
      "Token": {
        "ValidIssuer": "https://localhost:7000",
        "ValidAudience": "ServiceBricks",
        "ExpireMinutes": 1440,
        "SecretKey": "1111111111111111111111111111111111111111111111111111111111111111"
      }
    },
    "ServiceBus": {
      "Azure": {
        "ConnectionString": "Endpoint=",
        "Topic": "ServiceBricks",
        "Subscription": "NotificationSubscriptionExThree"
      }
    },
    "Storage": {
      "AzureDataTables": {
        "ConnectionString": "DefaultEndpointsProtocol="
      },
      "SqlServer": {
        "ConnectionString": "Data Source=localhost;Database=ExThree;Trusted_Connection=yes;Encrypt=false;"
      },
      "Sqlite": {
        "ConnectionString": "Data Source=c:\\temp\\ExThree.sqlite"
      },
      "Postgres": {
        "ConnectionString": "Server=localhost;Port=5432;Database=ExThree;User Id=postgres;Password=MyPassword"
      },
      "Cosmos": {
        "ConnectionString": "AccountEndpoint=",
        "Database": "ExThree"
      },
      "MongoDb": {
        "ConnectionString": "mongodb://localhost:27017",
        "Database": "ExThree"
      }
    }
  }
}